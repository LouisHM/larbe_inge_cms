<!DOCTYPE html>
<html class="" lang="{{ htmllang() }}">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        {# make sure we always display a proper title: The record's title if there is one, appended with the
           sitename. If there is no title, we append the sitename with the payoff, if there is one. #}
        <title>
            {%- if record.title is defined %}{{ record.title|striptags }} | {% endif -%}
                {{ config.get('general/sitename') -}}
            {% if record.title is not defined and config.get('general/payoff') %} | {{ config.get('general/payoff') }}{% endif -%}
        </title>
        <script src="https://kit.fontawesome.com/6a5880bc25.js" crossorigin="anonymous"></script>
        {{ encore_entry_link_tags('app', null, 'customAssets' ) }}
    </head>
    <body>

        {% block body %}

            {% block header %}
                {% include "partials/_header.twig" %}
            {% endblock header %}

            {% block main %}
                {# est contenu #}
            {% endblock main %}

            {% block footer %}
                {% include "partials/_footer.twig" %}
            {% endblock footer %}

        {% endblock body %}
        {{ encore_entry_script_tags('app', null, 'customAssets' ) }}
    </body>
</html>
